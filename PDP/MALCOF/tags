!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
State	malcof.c	/^typedef struct State{$/;"	s	file:
State	malcof.c	/^}State;$/;"	t	typeref:struct:State	file:
Suffix	malcof.c	/^typedef struct Suffix{$/;"	s	file:
Suffix	malcof.c	/^}Suffix;$/;"	t	typeref:struct:Suffix	file:
add	malcof.c	/^void add(char *prefix[], char *suffix)$/;"	f
build	malcof.c	/^void build(char *prefix[], FILE *f)$/;"	f
hash	malcof.c	/^unsigned int hash(char *s[])$/;"	f
lookup	malcof.c	/^State *lookup(char* prefix[], int create)$/;"	f
next	malcof.c	/^	State *next;$/;"	m	struct:State	file:
next	malcof.c	/^	Suffix *next;$/;"	m	struct:Suffix	file:
pref	malcof.c	/^	char *pref[NPREF];	$/;"	m	struct:State	file:
state_tab	malcof.c	/^State  *state_tab[NHASH];$/;"	v
suf	malcof.c	/^	Suffix *suf;$/;"	m	struct:State	file:
word	malcof.c	/^	char *word;$/;"	m	struct:Suffix	file:
